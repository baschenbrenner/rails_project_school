<ul>

    <h2>Course Title: <%= link_to_if(session[:user_type] == "teacher", course.title, "javascript:showCourse(#{course.teacher.id},#{course.id})") %></h2>

  <li>
    Catalog Number: <%= course.catalog_number  %>
  </li>
  <% if session[:user_type] == "teacher" %>
  <li>
    Description: <%= course.description  %>
  </li>
  <li>
    Teacher:
    <% if session[:user_type] == "student" %>
    <%= link_to course.teacher_name, teacher_path(course.teacher)  %>
    <% end %>
    <% if session[:user_type] == "teacher" %>
    <%= course.teacher_name %>
    <% end %>
  </li>
  <li>
    Room Number: <%= course.room_number %>
  </li>
  <li>
    Meeting Time and Days of the Week: <%= course.meeting_schedule_string %>
  </li>
  <ol>
    Essential Learnings for <%= course.title %>
    <% course.essential_learnings.each do |el| %>
    <li><%= el.name %> - <%= el.content %></li>
    <% end %>
    <% end %>
  </ol>




</ul>
